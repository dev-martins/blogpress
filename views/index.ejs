<%- include('partials/header'); -%>
  <div class="container">
    <img style="margin: auto;display: block;" src="https://miro.medium.com/max/1000/1*-_JN8MPndFy7Wksb90Rc_A.jpeg"
      class="img-fluid mt-4 mb-4" alt="...">
    <h3>Artigos</h3>
    <div class="row">
      <% articles.forEach(article=> { %>
        <div class="col-md-4">
          <div class="card mt-4">
            <img
              src="data:image/jpeg;base64,"
              class="card-img-top" alt="...">
            <div class="card-body">
              <h5 class="card-title">
                <%= article.title %>
              </h5>
              <p class="card-text">
                <%= article.body %>
              </p>
              <a href="/single/<%= article.slug %>" class="btn btn-primary">Leia</a>
            </div>
          </div>
        </div>
        <%})%>
    </div>

  </div>
  <%- include('partials/footer'); -%>